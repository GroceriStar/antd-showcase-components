    "@babel/preset-react": "7.7.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "25.1.0",
    "babel-loader": "8.0.6",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-preset-es2015": "6.24.1",
    "coveralls": "3.0.7",
